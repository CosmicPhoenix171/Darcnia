{
  "rules": {
    "characters": {
      "$characterId": {
        ".read": true,
        ".write": true
      }
    },
    "marketState": { ".read": true, ".write": true },
    "categoryAdjustments": { ".read": true, ".write": true },
    "vendorAdjustments": { ".read": true, ".write": true },

    "sessions": {
      "$sessionId": {
        "state": { ".read": true, ".write": true },
        "events": {
          ".read": true,
          "$eventId": { ".write": true }
        },
        "presence": {
          "$clientId": { ".read": true, ".write": true }
        }
      }
    }
  }
}
